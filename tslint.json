{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended", "tslint-config-prettier"],
	"jsRules": {},
	"rules": {
		"indent": [true, "tabs", 4],
		"array-type": [true, "array"],
		"member-access": false,
		"no-unused-expression": false,
		"variable-name": false,
		"object-literal-sort-keys": false,
		"no-console": false,
		"no-empty-interface": false
	},
	"rulesDirectory": []
}
